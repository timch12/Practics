        <TextBox x:Name="p" HorizontalAlignment="Left" Height="87" Margin="222,69,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="228" FontSize="22" FontWeight="Bold"/>
        <Button x:Name="pas" Content="Ввести пароль" HorizontalAlignment="Left" Height="81" Margin="222,166,0,0" VerticalAlignment="Top" Width="228" FontSize="22" FontWeight="Bold" Background="#FFCACCF4" Click="pas_Click"/>
        <TextBox x:Name="l1" HorizontalAlignment="Left" Height="258" Margin="456,69,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="266" FontSize="36" IsEnabled="False"/>

    </Grid>
</Window>
