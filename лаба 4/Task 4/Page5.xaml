<Window x:Class="Task_4.Page5"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Task_4"
        mc:Ignorable="d"
        Title="Page5" Height="450" Width="800">
    <Grid>
        <Button x:Name="Back" Content="В главное меню" HorizontalAlignment="Left" Height="71" Margin="56,58,0,0" VerticalAlignment="Top" Width="129" Click="Back_Click" BorderBrush="#FFE6C5C5" Background="#FFE8BFBF"/>
        <TextBox x:Name="num" HorizontalAlignment="Left" Height="48" Margin="338,129,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="94"/>
        <TextBlock HorizontalAlignment="Left" Height="55" Margin="317,69,0,0" TextWrapping="Wrap" Text="Введите число" VerticalAlignment="Top" Width="148" FontWeight="Bold" FontSize="20"/>
        <Button x:Name="button" Content="Вычислить" HorizontalAlignment="Left" Height="83" Margin="92,250,0,0" VerticalAlignment="Top" Width="156" Click="button_Click"/>
        <Label x:Name="ans" Content="" HorizontalAlignment="Left" Height="55" Margin="422,250,0,0" VerticalAlignment="Top" Width="134"/>

    </Grid>
</Window>
